dist:v_0-000_02
	a                 cpant:DeveloperRelease;
	a                 doap:Version;
	dc:identifier     "Type-Tiny-0.000_02"^^xsd:string;
	dc:issued         "2013-04-02"^^xsd:date;
	dcs:changeset     [
		dcs:item [
			a dcs:Documentation;
			rdfs:label "Beginnings of Type::Tiny::Manual.";
		];
